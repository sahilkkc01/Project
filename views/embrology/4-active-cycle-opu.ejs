<script>
    // Function to check readyForOpu status using Axios
    async function checkReadyForOpuStatus() {
        try {
            const response = await axios.get('http://localhost:5005/embrology/readyForOpuStatus');
            const readyForOpuStatus = response.data;

            // Log the API response for debugging purposes
            console.log('API Response:', readyForOpuStatus[0]);
            
            if (readyForOpuStatus[0] == 1) {
                // Remove the Modify and Vitrify buttons
                
                $('#opuDetails').prop('disabled',false);
                $('#Vitrify').prop('disabled',false);
            
            }else{ 
                $('#opuDetails').prop('disabled',true);
                $('#Vitrify').prop('disabled',true);
            }
        } catch (error) {
            console.error('Error fetching readyForOpu status:', error);
        }
    }

    // Call the function on page load or whenever necessary
    window.onload = checkReadyForOpuStatus;
</script>
