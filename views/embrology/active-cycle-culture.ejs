                    <script type="module">

                        document.getElementById('scheuleEtSubmit').addEventListener('click', function (event) {
                            event.preventDefault();

                            // Validate the form
                            const { isValid, errorMessage, formData } = validateForm('#scheduleEtForm');
                            const errorDivId = '#errordiv1';

                            if (!isValid) {
                                alert(errorMessage);
                                $(errorDivId).removeClass('alert-success').addClass('alert-danger').text(errorMessage).show();
                                return;
                            }

                            // Manually handle checkboxes
                            const fertStatus = document.getElementById('fertStatus').checked; // true or false
                            const etTimeERA = document.getElementById('etTimeERA').checked; // true or false

                            // Extract other form data
                            const etDate = formData.etDate;
                            const etTime = formData.etTime;
                            const reviewedBy = formData.reviewedBy; // You can replace 0 with the actual cycle ID if available

                            // Prepare the data object
                            const data = {
                                etDate,
                                etTime,
                                etTimeERA,
                                reviewedBy,
                                fertStatus,
                            };

                            // Send the form data using Axios
                            axios.post('/embrology/schedule-et/save', data)
                                .then(response => {
                                    if (response.data.success) {
                                        alert("Schedule ET saved successfully");
                                        $(errorDivId).removeClass('alert-danger').addClass('alert-success').text('Schedule ET saved successfully!').show();
                                    } else {
                                        alert("Error saving Schedule ET");
                                        $(errorDivId).removeClass('alert-success').addClass('alert-danger').text('Error saving Schedule ET').show();
                                    }
                                })
                                .catch(error => {
                                    alert("Error in saving Schedule ET");
                                    console.error('Error saving Schedule ET:', error);
                                    $(errorDivId).removeClass('alert-success').addClass('alert-danger').text('Error saving Schedule ET').show();
                                });
                        });
                    </script>
