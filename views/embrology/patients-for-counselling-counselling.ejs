                console.log("Document is ready");

                $("#formid").on("submit", function (event) {
                  event.preventDefault();
                  const { isValid, errorMessage, formData } =
                  validateForm("#formid");

                  console.log(formData);
                  const errorDivId = "#errordiv1";
                  let total = 0;
                  $('#registeration, #Day2, #Day6,#Day9, #Trigger,#Opu,#Donor, #et').each(function () {
                    total += parseInt($(this).val()) || 0; // Default to 0 if parsing fails
                  });

                  let amountData = {
                    registeration: $('#registerationAmount').val(),
                    Day2: $('#Day2Amount').val(),
                    Day6: $('#Day6Amount').val(),
                    Day9: $('#Day9Amount').val(),
                    Trigger: $('#TriggerAmount').val(),
                    Opu: $('#OpuAmount').val(),
                    Donor: $('#DonorAmount').val(),
                    et:$('#etAmount').val()
                  }

                  formData['amount'] = amountData;

                  if (isValid && total == 100) {

                    const url = "/embrology/post-patientCounselling";
                    const loadingDiv = "#loadingIndicator";
                    console.log(formData);
                    alert('Form Data is submitted')

                    myAjax(formData, errorDivId, url, loadingDiv);
                    location.reload();
                  } else {
                    alert('input field is not filled')
                    $(errorDivId)
                      .removeClass("alert-success")
                      .addClass("alert-danger")
                      .text(errorMessage)
                      .show();
                    $(errorDivId).text(errorMessage).show();
                  }
                });
              });
            </script>
