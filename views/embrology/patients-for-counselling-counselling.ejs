            <script>
              let amount;
              function fetchData() {
                axios.get('/embrology/get-package').then(res => {
                  let data = res.data;
                  data.forEach(element => {
                    $('#selectMany').append(`<option value='${element.packageRate}'>${element.packageName}</option>`);
                  });
                }).catch(err => {
                  console.log(err);
                })
              }
              $('#selectMany').on('change', () => {
                console.log()
                if ($('#selectMany').val() != '') {
                  $('#showCol').show()
                  amount = parseInt($('#selectMany').val());
                  $('#total').html(`
                    <p>total amount of package is ${$('#selectMany').val()}</p>
                    <input type='text' class='input-style' id='discount' placeholder='discount %'> 
                  `)
                } else {
                  $('#showCol').hide()
                  $('#total').html(`<p></p>`)
                }
              })
              $('#total').on('input','#discount',function (){
                let discount =   parseFloat($('#discount').val());
                let realAmount = parseFloat($('#selectMany').val());
                amount = amount - ((realAmount*discount)/100);
                console.log(amount)
                $('#total').html(`
                    <p>total amount of package is ${amount}</p>
                    <input id='discount' placeholder='discount value=${$('#discount').val()} %'> 
                  `)
              })
              $('#registeration,#total, #Day2, #Day6,#Day9, #Trigger,#Opu,#Donor, #et').on('input',function () {
                  console.log('amount')
                  let registerationAmount = parseInt($('#registeration').val())
                  let Day2Amount = parseInt($('#Day2').val())
                  let Day6Amount = parseInt($('#Day6').val())
                  let Day9Amount = parseInt($('#Day9').val())
                  let TriggerAmount = parseInt($('#Trigger').val())
                  let OpuAmount = parseInt($('#Opu').val())
                  let DonorAmount = parseInt($('#Donor').val())
                  let etAmount = parseInt($('#et').val())
                  $('#registerationAmount').val((Math.ceil((registerationAmount*amount)/100)));
                  $('#Day2Amount').val(Math.ceil((Day2Amount*amount)/100))
                  $('#Day6Amount').val(Math.ceil((Day6Amount*amount)/100))
                  $('#Day9Amount').val(Math.ceil((Day9Amount*amount)/100))
                  $('#TriggerAmount').val(Math.ceil((TriggerAmount*amount)/100))
                  $('#OpuAmount').val(Math.ceil((OpuAmount*amount)/100))
                  $('#DonorAmount').val(Math.ceil((DonorAmount*amount)/100))
                  $('#etAmount').val(Math.ceil((etAmount*amount)/100))
              });
              $('#registerationAmount, #Day2Amount, #Day6Amount,#Day9Amount, #TriggerAmount,#OpuAmount,#DonorAmount, #etAmount').on('input',function () {
                  console.log('amount')
                  let registerationAmount = parseInt($('#registerationAmount').val())
                  let Day2Amount = parseInt($('#Day2Amount').val())
                  let Day6Amount = parseInt($('#Day6Amount').val())
                  let Day9Amount = parseInt($('#Day9Amount').val())
                  let TriggerAmount = parseInt($('#TriggerAmount').val())
                  let OpuAmount = parseInt($('#OpuAmount').val())
                  let DonorAmount = parseInt($('#DonorAmount').val())
                  let etAmount = parseInt($('#etAmount').val())
                  $('#registeration').val((((registerationAmount/amount)*100).toFixed(2)));
                  $('#Day2').val(((Day2Amount/amount)*100).toFixed(2))
                  $('#Day6').val(((Day6Amount/amount)*100).toFixed(2))
                  $('#Day9').val(((Day9Amount/amount)*100).toFixed(2))
                  $('#Trigger').val(((TriggerAmount/amount)*100).toFixed(2))
                  $('#Opu').val(((OpuAmount/amount)*100).toFixed(2))
                  $('#Donor').val(((DonorAmount/amount)*100).toFixed(2))
                  $('#et').val(((etAmount/amount)*100).toFixed(2))
              });
              fetchData();
            </script>
