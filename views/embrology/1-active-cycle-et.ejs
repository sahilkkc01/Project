function fetchServicesData() {
  console.log(`Inside FetchServiceData`);
  axios
    .get("/embrology/ready-for-transfer2")
    .then((response) => {
      let data = response.data;
      console.log(`Data from ReadyToTransfer`, data);

      let tableRows = "";

      // Function to process each day's data and generate table rows
      const processDayData = (dayName, items) => {
        items.forEach((item) => {
          item.reportData.forEach((report) => {
            tableRows += `
              <tr>
                <td contenteditable="true"><input class="form-check-input mt-1" type="checkbox" id="gridCheck1"></td>
                <td contenteditable="true">${report.oocyteNo}</td>
                <td contenteditable="true">${report.embryoNo}</td>
                <td contenteditable="true">1</td>
                <td contenteditable="true">FRESH</td>
                <td contenteditable="true">${new Date(item.createdAt).toISOString().slice(0, 10)}</td>
                <td contenteditable="true">${report.grade}</td>
                <td contenteditable="true">${item.incubator}</td>
                <td contenteditable="true">YYV</td>
               
              </tr>
            `;
          });
        });
      };

      // Process each day's data and generate table rows
      if (data.emb_day3_ready_forTransfer) {
        processDayData("Day 3", data.emb_day3_ready_forTransfer);
      }
      if (data.embday4_readyforTransfer) {
        processDayData("Day 4", data.embday4_readyforTransfer);
      }
      if (data.embday5_readyforTransfer) {
        processDayData("Day 5", data.embday5_readyforTransfer);
      }
      if (data.embday6_readyforTransfer) {
        processDayData("Day 6", data.embday6_readyforTransfer);
      }

      // Append the rows to the table
      $("#tble101").html(tableRows);
    })
    .catch((error) => {
      console.error("Error fetching data:", error);
    });
}



