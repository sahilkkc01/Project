                          $(document).ready(function () {
                            console.log("Document is ready");
            
                            $("#formid").on("submit", function (event) {
                              event.preventDefault();
                              const { isValid, errorMessage, formData } =
                                validateForm("#formid");
            
                              // Initialize an array to hold the embryosTransferred data
                              const embryosTransferred = [];
            
                              // Iterate over each row in the table to find checked rows
                              $("#tble101 tr").each(function () {
                                const $row = $(this);
                                const isChecked = $row
                                  .find('input[type="checkbox"]')
                                  .is(":checked");
            
                                if (isChecked) {
                                  // Gather data from the checked row
                                  const oocyteNo = $row.find("td:eq(1)").text().trim();
                                  const embryoNo = $row.find("td:eq(2)").text().trim();
                                  const embryoAge = $row.find("td:eq(3)").text().trim();
                                  const freshFrozen = $row.find("td:eq(4)").text().trim();
                                  const fertilizationDate = $row
                                    .find("td:eq(5)")
                                    .text()
                                    .trim();
                                  const grade = $row.find("td:eq(6)").text().trim();
                                  const incubator = $row.find("td:eq(7)").text().trim();
                                  const addOn = $row.find("td:eq(8)").text().trim();
            
                                  // Add the row's data to the embryosTransferred array
                                  embryosTransferred.push({
                                    oocyteNo,
                                    embryoNo,
                                    embryoAge,
                                    freshFrozen,
                                    fertilizationDate,
                                    grade,
                                    incubator,
                                    addOn,
                                  });
                                }
                              });
            
                              console.log(
                                `EmbryosTransferred JSON IS:-`,
                                embryosTransferred
                              );
            
                              // Create a new object to store the form data
                              const finalFormData = {};
                              $.each(formData, function (key, value) {
                                finalFormData[key] = value;
                              });
            
                              // Ensure all checkboxes are included in the form data with the correct value
                              $('input[type="checkbox"]').each(function () {
                                const checkboxName = $(this).attr("name");
                                finalFormData[checkboxName] = $(this).is(":checked");
                              });
            
                              // Append the embryosTransferred data to finalFormData
                              finalFormData.embryosTransferred = embryosTransferred;
            
                              console.log(finalFormData);
                              const errorDivId = "#errordiv1";
            
                              if (isValid) {  
                                const url = "/embrology/embryo-transfer";
                                const loadingDiv = "#loadingIndicator";
            
                                console.log(finalFormData);
                                console.log(`This Data Submitted From Edit ET Page`);
                                
            
                                myAjax(finalFormData, errorDivId, url, loadingDiv);
                                fe();
                                $('#ETModel').modal('hide');
                                
                              } else {
                                $(errorDivId)
                                  .removeClass("alert-success")
                                  .addClass("alert-danger")
                                  .text(errorMessage)
                                  .show();
                              }
                            });
                          });
                          </script>

