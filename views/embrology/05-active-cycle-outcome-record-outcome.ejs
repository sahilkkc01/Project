                $('#submitForm').on('click', function () {
                        // Reference the file input element
                        var fileInput = $('#upload')[0];

                        // Check if files are selected
                        if (!fileInput.files || fileInput.files.length === 0) {
                            console.error('No file selected');
                            return;
                        }
                    
                        var file = fileInput.files[0];
                        var customSuffix = 'outcome-' + Date.now(); // Example custom suffix
                    
                        var formData = new FormData();
                        formData.append('customSuffix', customSuffix);
                        formData.append('image', file);
                        console.log(formData)
                        axios
                        .post('/embrology/upload-img', formData, {
                            headers: { 'Content-Type': 'multipart/form-data' }
                        })
                        .then((response) => {
                            let data = response.data;
                            console.log(data)
                        })
                        .catch((error) => {
                            console.error("Error uploading files:", error);
                        });// Ensure the URL matches your server route
                });

</script>
