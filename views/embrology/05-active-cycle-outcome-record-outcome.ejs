                      <script>
                        $(document).ready(function () {
    // Hide Clinical Miscarriage, Ongoing Pregnancy, and Birth Details sections by default
    $('#clinicalMiscarriage').addClass('hidden');
    $('#ongoingPregnancy').addClass('hidden');
    $('#birthDetails').addClass('hidden');

    // Toggle visibility based on isClinicalMiscarriage checkbox
    $('#isclinicalMiscariage').change(function () {
        if ($(this).is(':checked')) {
            $('#clinicalMiscarriage').removeClass('hidden');
            $('#ongoingPregnancy').addClass('hidden');
            $('#birthDetails').addClass('hidden');
        } else {
            const value = $('#noOfSac').val();
            if (value !== '0' && value !== '') {
                $('#ongoingPregnancy').removeClass('hidden');
                $('#birthDetails').removeClass('hidden');
                $('#clinicalMiscarriage').addClass('hidden');
            } else {
                $('#ongoingPregnancy').addClass('hidden');
                $('#birthDetails').addClass('hidden');
            }
            $('#clinicalMiscarriage').addClass('hidden');
        }
    });

    // Toggle visibility based on noOfSac dropdown
    $('#noOfSac').change(function () {
        const value = $(this).val();
        if (value !== '0' && value !== '') {
            $('#ongoingPregnancy').removeClass('hidden');
            $('#birthDetails').removeClass('hidden');
            $('#clinicalMiscarriage').addClass('hidden');
        } else {
            $('#ongoingPregnancy').addClass('hidden');
            $('#birthDetails').addClass('hidden');
        }
    });

    // Add a new Beta HCG row
    $('#addBetaHcgRow').click(function () {
        const row = `
            <div class="row betaHcgRow p-2 ">
                <div class="col-lg-3 col-md-4 col-sm-12">
                    <div class="form-group">
                        <label class="input__label">Beta HCG Value</label>
                        <input name="betaHcgValue" type="number" class="form-control text-right input-style" placeholder="">
                    </div>
                </div>
                <div class="col-lg-3 col-md-4 col-sm-12">
                    <div class="form-group">
                        <label class="input__label">Date</label>
                        <input name="betaHcgDate" type="date" class="form-control text-right input-style" placeholder="">
                    </div>
                </div>
                <div class="col-lg-3 col-md-4 col-sm-12">
                    <div class="form-group">
                        <label class="input__label">Recorded By</label>
                        <select name="recordedBy" class="form-control input-style">
                            <option></option>
                            <option>DR. NISHA BHATNAGAR</option>
                            <option>Assistant Doctor</option>
                        </select>
                    </div>
                </div>
                <div class="col-lg-3 col-md-4 col-sm-12">
                    <div class="form-group">
                        <label class="input__label">Observations</label>
                        <textarea name="observations" class="form-control input-style" placeholder=""></textarea>
                    </div>
                </div>
                <div class="col-lg-12 text-right">
                    <button type="button" class="btn btn-danger deleteRowBtn mt-2">
                        <strong><i class="fas fa-times"></i> Delete</strong>
                    </button>
                </div>
            </div>
        `;
        $('#betaHcgContainer').append(row);
    });

    // Delegate event handling to dynamically added delete buttons
    $('#betaHcgContainer').on('click', '.deleteRowBtn', function () {
        $(this).closest('.betaHcgRow').remove();
    });

    // Submit form data
    $('#submitForm').click(function () {
        validateForm();
    });

    function validateForm() {
        const betaHcgData = [];

        $('#betaHcgContainer .betaHcgRow').each(function () {
            const row = {
                betaHcgValue: $(this).find('input[name="betaHcgValue"]').val(),
                betaHcgDate: $(this).find('input[name="betaHcgDate"]').val(),
                recordedBy: $(this).find('select[name="recordedBy"]').val(),
                observations: $(this).find('textarea[name="observations"]').val(),
            };
            betaHcgData.push(row);
        });

        const formData = {
            clinicalPregnancyDate: $('#clinicalPregnancyDate').val() || '',
            clinicalPregnancyRecordedBy: $('#clinicalPregnancyRecordedBy').val() || '',
            clinicalPregnancyLocation: $('#clinicalPregnancyLocation').val() || '',
            noOfSac: $('#noOfSac').val() || '',
            clinicalPregnancyObservations: $('#clinicalPregnancyObservations').val() || '',
            ongoingPregnancyObservations: $('#ongoingPregnancyObservations').val() || '',
            miscarriageDate: $('#miscarriageDate').val() || '',
            miscarriageTime: $('#miscarriageTime').val() || '',
            abortionType: $('input[name="abortionType"]:checked').val() || '',
            clinicalMiscarriageObservations: $('#clinicalMiscarriageObservations').val() || '',
            deliveryDate: $('#deliveryDate').val() ,
            birthRecordedBy: $('#birthRecordedBy').val() || '',
            birthType: $('input[name="birthType"]:checked').val() || '',
            modeOfDelivery: $('input[name="modeOfDelivery"]:checked').val() || '',
            neonatalComplications: $('#neonatalComplications').val() || '',
            postPartumMaternalComplications: $('#postPartumMaternalComplications').val() || '',
            birthDetailsObservations: $('#birthDetailsObservations').val() || '',
            betaHcgData: betaHcgData,
        };

        console.log(formData)
        // Send the data to the backend using Axios
        axios.post('/embrology/submitOutcome', formData)
            .then(function (response) {
                alert('Data submitted successfully!');
            })
            .catch(function (error) {
                alert('Error submitting data.');
            });
    }
});

</script>
