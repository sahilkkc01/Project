            <!-- content -->
            <div class="container-fluid">
                <div id="errordiv1"></div>
                <form id="patientReg" method="post">
                    <div class="chart">
                        <div class="row">
                            <div class="col-lg-12 pl-lg-0 mt-2 ml-2">
                                <h4 class="primary">Patient Registration</h4>
                            </div>
                            <div class="col-lg-12 pl-lg-2 chart-grid">
                                <div class="card card_border">
                                    <div class="">
                                        <!--Past your code here starts-->
                                        <div class="card">
                                            <div class="card-body pb-1 pt-2">
                                                <div class="form-row">
                                                    <!-- Date Field -->
                                                    <div class="col-lg-3 col-md-4 col-sm-12 mb-2">
                                                        <div class="form-group">
                                                            <label class="input__label">Date</label>
                                                            <input data-validation="required"
                                                                value="<%= patient.date ? patient.date : currentDate %>"
                                                                disabled data-error-message="Date is required" id="date"
                                                                name="date" type="date" class="form-control input-style"
                                                                placeholder="" <%=patient.id ? 'disabled' : '' %>
                                                            />

                                                        </div>
                                                    </div>

                                                    <!-- Registration Type Field -->
                                                    <div class="col-lg-3 col-md-4 col-sm-12 mb-2">
                                                        <div class="form-group">
                                                            <label class="input__label">Registration Type</label>
                                                            <select data-validation="required"
                                                                data-error-message="Registration Type is required"
                                                                name="RegistrationType" id="RegistrationType"
                                                                class="form-control input-style" <%=patient.id
                                                                ? 'disabled' : '' %>>
                                                                <option value="Select" <%=!patient.RegistrationType ||
                                                                    patient.RegistrationType==='Select' ? 'selected'
                                                                    : '' %>>Select</option>
                                                                <option value="Single"
                                                                    <%=patient.RegistrationType==='Single' ? 'selected'
                                                                    : '' %>>Single</option>
                                                                <option value="Couple"
                                                                    <%=patient.RegistrationType==='Couple' ? 'selected'
                                                                    : '' %>>Couple</option>
                                                                <option value="EggDonor"
                                                                    <%=patient.RegistrationType==='EggDonor'
                                                                    ? 'selected' : '' %>>Egg Donor</option>
                                                                <option value="SpermDonor"
                                                                    <%=patient.RegistrationType==='SpermDonor'
                                                                    ? 'selected' : '' %>>Sperm Donor</option>
                                                                <option value="SurrogateMother"
                                                                    <%=patient.RegistrationType==='SurrogateMother'
                                                                    ? 'selected' : '' %>>Surrogate Mother</option>

                                                            </select>
                                                        </div>
                                                    </div>

                                                    <!-- MR No. Field -->
                                                    <div class="col-lg-3 col-md-4 col-sm-12 mb-2">
                                                        <div class="form-group">
                                                            <label class="input__label">MR No.</label>
                                                            <input data-validation="required"
                                                                value="<%= patient.mr_no %>"
                                                                data-error-message="MR No. is required" id="mr_no"
                                                                name="mr_no" type="number"
                                                                class="form-control input-style" placeholder=""
                                                                <%=patient.id ? 'disabled' : '' %>
                                                            />
                                                        </div>
                                                    </div>

                                                    <!-- Agency Name Field -->
                                                    <div class="col-lg-3 col-md-4 col-sm-12 mb-2">
                                                        <div class="form-group">
                                                            <label class="input__label">ART BANK NAME</label>
                                                            <select data-validation="required"
                                                                data-error-message="ArtBankName is required"
                                                                name="ArtBankName" id="ArtBankName"
                                                                class="form-control input-style" <%=patient.id
                                                                ? 'disabled' : '' %>>
                                                                <option value="Select" <%=!patient.ArtBankName ||
                                                                    patient.ArtBankName==='Select' ? 'selected' : '' %>
                                                                    >Select</option>
                                                                <option value="1" <%=patient.ArtBankName==='1'
                                                                    ? 'selected' : '' %>>1</option>
                                                                <option value="2" <%=patient.ArtBankName==='2'
                                                                    ? 'selected' : '' %>>2</option>
                                                            </select>
                                                        </div>
                                                    </div>

                                                    <!-- Agent Field -->
                                                    <div class="col-lg-3 col-md-4 col-sm-12 mb-2">
                                                        <div class="form-group">
                                                            <label class="input__label">Agent</label>
                                                            <select data-validation="required"
                                                                data-error-message="Agent is required" id="Agent"
                                                                name="Agent" class="form-control input-style"
                                                                <%=patient.id ? 'disabled' : '' %>>
                                                                <option value="Select" <%=!patient.Agent ||
                                                                    patient.Agent==='Select' ? 'selected' : '' %>>Select
                                                                </option>
                                                                <option value="1" <%=patient.Agent==='1' ? 'selected'
                                                                    : '' %>>1</option>
                                                                <option value="2" <%=patient.Agent==='2' ? 'selected'
                                                                    : '' %>>2</option>
                                                            </select>
                                                        </div>
                                                    </div>

                                                    <!-- Source Of Reference Field -->
                                                    <div class="col-lg-3 col-md-4 col-sm-12 mb-2">
                                                        <div class="form-group">
                                                            <label class="input__label">Source Of Reference</label>
                                                            <select data-validation="required"
                                                                data-error-message="Source of Reference is required"
                                                                id="SorOfRef" name="SorOfRef"
                                                                class="form-control input-style" <%=patient.id
                                                                ? 'disabled' : '' %>>
                                                                <option value="" <%=!patient.SorOfRef ? 'selected' : ''
                                                                    %>>- Select -</option>
                                                                <option value="CAMP" <%=patient.SorOfRef==='CAMP'
                                                                    ? 'selected' : '' %>>CAMP</option>
                                                                <option value="COORDINATOR"
                                                                    <%=patient.SorOfRef==='COORDINATOR' ? 'selected'
                                                                    : '' %>>COORDINATOR</option>
                                                                <option value="EMPLOYEE REFERRAL"
                                                                    <%=patient.SorOfRef==='EMPLOYEE REFERRAL'
                                                                    ? 'selected' : '' %>>EMPLOYEE REFERRAL</option>
                                                                <option value="EXTERNAL DOCTOR REFERRAL"
                                                                    <%=patient.SorOfRef==='EXTERNAL DOCTOR REFERRAL'
                                                                    ? 'selected' : '' %>>EXTERNAL DOCTOR REFERRAL
                                                                </option>
                                                                <option value="FACILITATOR"
                                                                    <%=patient.SorOfRef==='FACILITATOR' ? 'selected'
                                                                    : '' %>>FACILITATOR</option>
                                                                <option value="FAMILY & FRIENDS REFERRAL"
                                                                    <%=patient.SorOfRef==='FAMILY & FRIENDS REFERRAL'
                                                                    ? 'selected' : '' %>>FAMILY & FRIENDS REFERRAL
                                                                </option>
                                                                <option value="HOARDINGS"
                                                                    <%=patient.SorOfRef==='HOARDINGS' ? 'selected' : ''
                                                                    %>>HOARDINGS</option>
                                                                <option value="INTERNAL DOCTOR REFERRAL"
                                                                    <%=patient.SorOfRef==='INTERNAL DOCTOR REFERRAL'
                                                                    ? 'selected' : '' %>>INTERNAL DOCTOR REFERRAL
                                                                </option>
                                                                <option value="MARKETING"
                                                                    <%=patient.SorOfRef==='MARKETING' ? 'selected' : ''
                                                                    %>>MARKETING</option>
                                                                <option value="NEWSPAPER_AND_MAGAZINE_ADS_ARTICLES"
                                                                    <%=patient.SorOfRef==='NEWSPAPER_AND_MAGAZINE_ADS_ARTICLES'
                                                                    ? 'selected' : '' %>>NEWSPAPER AND MAGAZINE
                                                                    ADS/ARTICLES</option>
                                                                <option value="OTHERS" <%=patient.SorOfRef==='OTHERS'
                                                                    ? 'selected' : '' %>>OTHERS</option>
                                                                <option value="SELF" <%=patient.SorOfRef==='SELF'
                                                                    ? 'selected' : '' %>>SELF</option>
                                                                <option value="STAFF_REFERRAL"
                                                                    <%=patient.SorOfRef==='STAFF_REFERRAL' ? 'selected'
                                                                    : '' %>>STAFF REFERRAL</option>
                                                                <option value="WEB_AND_EMAIL"
                                                                    <%=patient.SorOfRef==='WEB_AND_EMAIL' ? 'selected'
                                                                    : '' %>>WEB AND EMAIL</option>
                                                                <option value="WORD_OF_MOUTH"
                                                                    <%=patient.SorOfRef==='WORD_OF_MOUTH' ? 'selected'
                                                                    : '' %>>WORD OF MOUTH</option>
                                                            </select>
                                                        </div>
                                                    </div>

                                                    <!-- Camp Field -->
                                                    <div class="col-lg-3 col-md-4 col-sm-12 mb-2">
                                                        <div class="form-group">
                                                            <label class="input__label">Camp</label>
                                                            <select data-validation="required"
                                                                data-error-message="Camp is required" id="camp"
                                                                name="camp" class="form-control input-style"
                                                                <%=patient.id ? 'disabled' : '' %>>
                                                                <option value="" <%=!patient.camp ? 'selected' : '' %>
                                                                    >Select</option>
                                                                <option value="1" <%=patient.camp==='1' ? 'selected'
                                                                    : '' %>>1</option>
                                                                <option value="2" <%=patient.camp==='2' ? 'selected'
                                                                    : '' %>>2</option>
                                                            </select>
                                                        </div>
                                                    </div>

                                                    <!-- Referral Details Field -->
                                                    <div class="col-lg-3 col-md-4 col-sm-12 mb-2">
                                                        <div class="form-group">
                                                            <label class="input__label">Referral Details</label>
                                                            <input data-validation="required"
                                                                value="<%= patient.ReferralDetails %>"
                                                                data-error-message="Referral Details is required"
                                                                id="ReferralDetails" name="ReferralDetails" type="text"
                                                                class="form-control input-style" placeholder=""
                                                                <%=patient.id ? 'disabled' : '' %>
                                                            />
                                                        </div>
                                                    </div>

                                                    <!-- Doctor Type Radio Buttons -->
                                                    <div class="col-lg-3 col-md-4 col-sm-12">
                                                        <div class="form-check-label form-row">
                                                            <div class="radio" data-validation="required"
                                                                data-error-message="Select one of the options">
                                                                <label
                                                                    class="form-check-label d-flex align-items-center"
                                                                    for="externalDoctor">
                                                                    <input id="externalDoctor" name="doctorType"
                                                                        class="form-check-input" type="radio"
                                                                        value="External Doctor"
                                                                        <%=patient.doctorType==='External Doctor'
                                                                        ? 'checked' : '' %>
                                                                    <%= patient.id ? 'disabled' : '' %>
                                                                        />
                                                                        External Doctor
                                                                </label>
                                                                <label
                                                                    class="form-check-label d-flex align-items-center mt-2"
                                                                    for="inHouseDoctor">
                                                                    <input id="inHouseDoctor" name="doctorType"
                                                                        class="form-check-input" type="radio"
                                                                        value="Inhouse Doctor"
                                                                        <%=patient.doctorType==='Inhouse Doctor'
                                                                        ? 'checked' : '' %>
                                                                    <%= patient.id ? 'disabled' : '' %>
                                                                        />
                                                                        Inhouse Doctor
                                                                </label>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <!-- Clinic Status Field -->
                                                    <div class="col-lg-3 col-md-4 col-sm-12 mb-2 mt-4">
                                                        <div class="form-group">
                                                            <select id="clinic_status" name="clinic_status"
                                                                class="form-control input-style" <%=patient.id
                                                                ? 'disabled' : '' %>>
                                                                <option value="" <%=!patient.clinic_status ? 'selected'
                                                                    : '' %>>Select</option>
                                                                <option value="1" <%=patient.clinic_status==='1'
                                                                    ? 'selected' : '' %>>1</option>
                                                                <option value="2" <%=patient.clinic_status==='2'
                                                                    ? 'selected' : '' %>>2</option>
                                                            </select>
                                                        </div>
                                                    </div>

                                                    <!-- Referral Doctor Button -->
                                                    <div class="col-lg-3 col-md-4 col-sm-12">
                                                        <div class="form-group">
                                                            <button data-bs-toggle="modal" data-bs-target="#myModal"
                                                                type="button"
                                                                class="btn btn-style mt-4 btn-secondary btn-style btn-change"
                                                                <%=patient.id ? 'disabled' : '' %>>
                                                                <strong><i class="fa-solid fa-stethoscope"></i> Referral
                                                                    Doctor</strong>
                                                            </button>
                                                        </div>
                                                    </div>
                                                </div>


                                            </div>
                                            <!--Past your code here ends-->
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <nav>
                                    <div class="nav nav-tabs" id="nav-tab" role="tablist">
                                        <button class="nav-link active" id="nav-followup-tab" data-bs-toggle="tab"
                                            data-bs-target="#nav-followup" type="button" role="tab"
                                            aria-controls="nav-followup" aria-selected="true">
                                            <i class="fas fa-exchange-alt"></i> Patient Details
                                        </button>
                                        <button class="nav-link" id="nav-appointment-tab" data-bs-toggle="tab"
                                            data-bs-target="#nav-appointment" type="button" role="tab"
                                            aria-controls="nav-appointment" aria-selected="false">
                                            <i class="fas fa-calendar-alt"></i> Spouse Details
                                        </button>
                                        <button class="nav-link" id="nav-visited-tab" data-bs-toggle="tab"
                                            data-bs-target="#nav-visited" type="button" role="tab"
                                            aria-controls="nav-visited" aria-selected="false">
                                            <i class="fas fa-map-marker-alt"></i> Sponsor and Bank
                                            Details
                                        </button>
                                    </div>
                                </nav>

                                <div class="col-md-12 tab-content" id="nav-tabContent">
                                    <div class="tab-pane fade show active" id="nav-followup" role="tabpanel"
                                        aria-labelledby="nav-followup-tab">
                                        <div class="col-lg-12 pl-lg-2 pr-lg-3">
                                            <div class="card card_border">
                                                <div class="col-xl-12 p-heading">
                                                    <!-- Nav Tabs are here once have a look--->
                                                    <h6 class="p-text">PATIENT INFORMATION</h6>
                                                </div>
                                                <div class="row pl-3 pr-3">
                                                    <div class="card-body col-3 pr-0">
                                                        <div class="form-row">
                                                            <div class="col-lg-12 col-md-12 col-sm-12 mb-2 height-50%">
                                                                <div class="form-group">
                                                                    <video id="video" width="300" height="400"
                                                                        autoplay></video>
                                                                    <img id="captured-image" name="capturedImg"
                                                                        style="display: none" width="300"
                                                                        height="200" />
                                                                    <canvas id="canvas" width="300" height="400"
                                                                        style="display: none"></canvas>
                                                                    <img src="" alt="qrCode" id="qr" style="display: none;">
                                                                </div>
                                                            </div>
                                                            <div class="col-lg-6 col-md-6 col-sm-12 mb-2">
                                                                <div class="form-group text-right pr-4">
                                                                    <button type="button" id="upload"
                                                                        onclick="document.getElementById('patientImg').click()">
                                                                        <i
                                                                            class="fa-solid fa-arrow-up-from-bracket"></i>
                                                                    </button>
                                                                    <input type="file" id="patientImg" name="patientImg"
                                                                        style="display: none" />
                                                                </div>
                                                            </div>
                                                            <div class="col-lg-6 col-md-6 col-sm-12 mb-2">
                                                                <div class="form-group text-left pl-4">
                                                                    <button type="button" id="capture">
                                                                        <i class="fa-solid fa-camera"></i>
                                                                    </button>
                                                                </div>
                                                            </div>
                                                            <div class="col-lg-6 col-md-6 col-sm-12 mb-2">
                                                                <div class="form-group text-left pl-4">
                                                                    <button type="button" id="qrCode">
                                                                        <i class="fa-solid fa-qrcode"></i>
                                                                    </button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="card-body col-9 pt-2">
                                                        <div class="form-row">
                                                            <!-- Prefix -->
                                                            <div class="col-lg-4 col-md-4 col-sm-12 mb-2">
                                                                <div class="form-group">
                                                                    <label class="input__label"
                                                                        for="prefix">Prefix</label>
                                                                    <select data-validation="required"
                                                                        data-error-message="Prefix is required"
                                                                        id="prefix" name="prefix"
                                                                        class="form-control input-style prefix"
                                                                        <%=patient.id ? 'disabled' : '' %>>
                                                                        <option value="">Select</option>
                                                                        <option value="1" <%=patient.prefix==='1'
                                                                            ? 'selected' : '' %>>1</option>
                                                                        <option value="2" <%=patient.prefix==='2'
                                                                            ? 'selected' : '' %>>2</option>
                                                                    </select>
                                                                </div>
                                                            </div>

                                                            <!-- First Name -->
                                                            <div class="col-lg-4 col-md-4 col-sm-12 mb-2">
                                                                <div class="form-group">
                                                                    <label class="input__label" for="firstName">First
                                                                        Name</label>
                                                                    <input data-validation="required"
                                                                        data-error-message="First name is required"
                                                                        id="firstName" name="firstName" type="text"
                                                                        class="form-control input-style" placeholder=""
                                                                        value="<%= patient.firstName %>" <%=patient.id
                                                                        ? 'disabled' : '' %>/>
                                                                </div>
                                                            </div>

                                                            <!-- Middle Name -->
                                                            <div class="col-lg-4 col-md-4 col-sm-12 mb-2">
                                                                <div class="form-group">
                                                                    <label class="input__label" for="middleName">Middle
                                                                        Name</label>
                                                                    <input data-validation="required"
                                                                        data-error-message="Middle name is required"
                                                                        id="middleName" name="middleName" type="text"
                                                                        class="form-control input-style" placeholder=""
                                                                        value="<%= patient.middleName %>" <%=patient.id
                                                                        ? 'disabled' : '' %>/>
                                                                </div>
                                                            </div>

                                                            <!-- Last Name -->
                                                            <div class="col-lg-4 col-md-4 col-sm-12 mb-2">
                                                                <div class="form-group">
                                                                    <label class="input__label" for="lastName">Last
                                                                        Name</label>
                                                                    <input data-validation="required"
                                                                        data-error-message="Last name is required"
                                                                        id="lastName" name="lastName" type="text"
                                                                        class="form-control input-style" placeholder=""
                                                                        value="<%= patient.lastName %>" <%=patient.id
                                                                        ? 'disabled' : '' %>/>
                                                                </div>
                                                            </div>

                                                            <!-- Family Name -->
                                                            <div class="col-lg-4 col-md-4 col-sm-12 mb-2">
                                                                <div class="form-group">
                                                                    <label class="input__label" for="familyName">Family
                                                                        Name</label>
                                                                    <input data-validation="required"
                                                                        data-error-message="Family name is required"
                                                                        id="familyName" name="familyName" type="text"
                                                                        class="form-control input-style" placeholder=""
                                                                        value="<%= patient.familyName %>" <%=patient.id
                                                                        ? 'disabled' : '' %>/>
                                                                </div>
                                                            </div>

                                                            <!-- Father's Name -->
                                                            <div class="col-lg-4 col-md-4 col-sm-12 mb-2">
                                                                <div class="form-group">
                                                                    <label class="input__label"
                                                                        for="fatherName">Father's Name</label>
                                                                    <input data-validation="required"
                                                                        data-error-message="Father's name is required"
                                                                        id="fatherName" name="fatherName" type="text"
                                                                        class="form-control input-style" placeholder=""
                                                                        value="<%= patient.fatherName %>" <%=patient.id
                                                                        ? 'disabled' : '' %>/>
                                                                </div>
                                                            </div>

                                                            <div class="col-lg-4 col-md-4 col-sm-12 mb-2">
                                                                <div class="form-group">
                                                                    <label class="input__label"
                                                                        for="Gender">Gender</label>
                                                                    <select data-validation="required"
                                                                        data-error-message="Gender is required"
                                                                        id="Gender" name="Gender"
                                                                        class="form-control input-style " <%=patient.id
                                                                        ? 'disabled' : '' %>>
                                                                        <option value="">Select</option>
                                                                        <option value="Male" <%=patient.Gender==='Male'
                                                                            ? 'selected' : '' %>>Male</option>
                                                                        <option value="Female"
                                                                            <%=patient.Gender==='Female' ? 'selected'
                                                                            : '' %>>Female</option>
                                                                        <option value="Other"
                                                                            <%=patient.Gender==='Other' ? 'selected'
                                                                            : '' %>>Other</option>
                                                                    </select>
                                                                </div>
                                                            </div>

                                                            <!-- Blood Group -->
                                                            <div class="col-lg-4 col-md-4 col-sm-12 mb-2">
                                                                <div class="form-group">
                                                                    <label class="input__label" for="bloodGroup">Blood
                                                                        Group</label>
