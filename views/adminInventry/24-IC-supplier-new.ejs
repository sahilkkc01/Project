        const filteredCities = data.filter(city => city.stateCode === a.value);
        
        filteredCities.forEach(function(city) {
            selectOptions += `
            <option value="${city.cityName}">${city.cityName}</option>
            `;
        });
        
        $('#city').html(selectOptions); // Populate select options
