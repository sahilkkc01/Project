<script type="module">
     import {pagi_search} from '../javascripts/pagi_search.js'
  $(document).ready(function() {
  // Function to fetch and display data
  function fetchData() {
    $.ajax({
      url: '/clinic/get-city-details',
      type: 'GET',
      success: function(data) {
        let tableRows = '';
        data.reverse();
                  data.forEach(function(item) {
                    tableRows += `
                      <tr data-city-id="${item.id}">
    <td><button type="button" id="view_btn" class="btn btn-primary btn-style btn-change">View</button></td>
    <td>${item.city_master_code}</td>
    <td>${item.city}</td>
    <td>${item.state}</td>
    <td>${item.country}</td>
    
    <td><input class="form-check-input mt-0" onclick="updateStatus('${item.id}', this.checked,'<%= a %>',','clinic')" type="checkbox" type="checkbox" ${item.status ? 'checked' : ''}></td>
  </tr>
                    `;
                  });
                  $('#city-tbl').on('click', '#view_btn', function() {
                    const id=$(this).closest('tr').data('city-id')
                      const route='39'
                      const module='clinic'
                      ViewBtn(route,id,module)         // window.location.href = '/clinic/8' 
                     });
       
        $('#city-tbl').html(tableRows);
        pagi_search('#searchBox','#tableid','#paginationControls')
      },
      error: function(xhr, status, error) {
        console.error('Error fetching classification details:', error);
      }
    });
  }

  // Fetch data on page load
  fetchData();
});
</script>

