<p id="pgload"></p> <!-- This is the paragraph where we will display the values -->

<script>
    window.onload = function() {
        // Use setTimeout to ensure that loadEventEnd has time to register
        setTimeout(function() {
            // Performance timings
            const performanceTiming = window.performance.timing;

            // Ensure loadEventEnd is not 0 before calculating load time
            if (performanceTiming.loadEventEnd > 0) {
                // Calculate loading time
                const pageLoadTime = performanceTiming.loadEventEnd - performanceTiming.navigationStart;
                const pageRenderTime = performanceTiming.domComplete - performanceTiming.domLoading;

                // Calculate total page size
                let pageSize = 0;
                window.performance.getEntriesByType('resource').forEach(resource => {
                    if (resource.transferSize) {
                        pageSize += resource.transferSize; // transferSize gives the size of resource in bytes
                    }
                });

                // Convert page size to KB/MB
                let pageSizeInKB = (pageSize / 1024).toFixed(2); // KB
                let pageSizeInMB = (pageSize / (1024 * 1024)).toFixed(2); // MB if needed

                // Prepare the text content
                let displayText = `
                    Page Load Time: ${pageLoadTime} ms<br>
                    Page Render Time: ${pageRenderTime} ms<br>
                    Total Page Size: ${pageSizeInKB} KB (${pageSizeInMB} MB)
                `;

                // Output the results into the <p> tag with id "pgload"
                document.getElementById('pgload').innerHTML = displayText;
            } else {
                document.getElementById('pgload').innerHTML = "Page Load Time data is not available.";
            }
        }, 0); // Delay execution to ensure all resources are fully loaded
    };
</script>
